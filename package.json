    "postbuild": "npm run build:server",
    "build:start": "npm run build && npm run start:api",
    "build:daemon": "node scripts/build-daemon.cjs",
    "build:deploy": "npm run build && npm run build:daemon",
    "pm2:start": "node scripts/start-pm2-instances.cjs",
