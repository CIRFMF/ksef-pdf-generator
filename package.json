  "scripts": {
    "dev": "vite --mode public --config vite.config.ts",
    "start:api": "node --max-old-space-size=3072 dist/api/server.cjs",
    "dev:api": "ts-node --esm -P tsconfig.server.json src/api/server.ts",
    "build:server": "esbuild src/api/server.ts --bundle --platform=node --target=node18 --format=cjs --outfile=dist/api/server.cjs --sourcemap --external:swagger-ui-express --external:swagger-ui-dist",
