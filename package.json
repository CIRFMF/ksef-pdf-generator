    "pm2:status": "pm2 status",
    "type": "tsc -p src/app-public/tsconfig.json --noEmit",
    "test": "vitest",
    "test:api": "node scripts/test-generate-invoice.mjs",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --coverage",
    "install:service": "node scripts/install-service.cjs",
    "uninstall:service": "node scripts/uninstall-service.cjs",
    "cleanup:logs": "node scripts/cleanup-logs.cjs"
  },
  "main": "./ksef-fe-invoice-converter.umd.js",
  "module": "./ksef-fe-invoice-converter.es.js",
  "types": "./src/lib/index.d.ts",
